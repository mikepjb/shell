{
  "$schema": "https://opencode.ai/config.json",
  "disabled_providers": [
    "openai",
    "anthropic",
    "google",
    "groq",
    "azure",
    "openrouter"
  ],
  "model": "llama/qwen3-coder",
  "provider": {
    "llama": {
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "http://localhost:9090/v1",
        "apiKey": "none"
      },
      "models": {
        "qwen3-coder": {
          "id": "qwen3-coder"
        }
      }
    }
  },
  "mode": {
    "build": {
      "model": "llama/qwen3-coder",
      "prompt": "{file:./AGENTS.md}",
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    },
    "plan": {
      "model": "llama/qwen3-coder",
      "prompt": "{file:./AGENTS.md}",
      "tools": {
        "write": false,
        "edit": false,
        "bash": false
      }
    }
  }
}
